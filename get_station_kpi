def get_station_kpi(station_code):
    url = f"https://{DOMAIN}/thirdData/getStationRealKpi"
    res = session.post(
        url,
        headers={"X-Access-Token": token},
        json={"stationCodes": [station_code]}
    )
    res.raise_for_status()
    data = res.json()
    print("DEBUG station:", station_code, "â†’ respuesta:", data)
    return data

